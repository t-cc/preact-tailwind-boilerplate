{
  "name": "preact-tailwind-boilerplate",
  "version": "2.0.2",
  "license": "MIT",
  "source": "src/index.html",
  "scripts": {
    "start": "TAILWIND_MODE=watch parcel serve src/index.html --open",
    "start:clean": "(rm -r dist || true && rm -r .parcel-cache || true) && parcel serve src/index.html",
    "build": "rm -r dist && parcel build src/index.html"
  },
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "targets": {
    "app": {}
  },
  "dependencies": {
    "parcel": "^2.3.2",
    "preact": "^10.6.6",
    "postcss": "^8.4.7",
    "tailwindcss": "^3.0.23",
    "autoprefixer": "^10.4.2",
    "@parcel/packager-raw-url": "^2.3.2",
    "@parcel/transformer-typescript-tsc": "^2.3.2",
    "@parcel/transformer-webmanifest": "^2.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "typescript": "^4.6.2",
    "@parcel/validator-typescript": "^2.3.2"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  }
}
